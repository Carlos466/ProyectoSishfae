<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Crear Area</title>


    <style type="text/css">
    </style>



</head>
<body>


    <div class="container">


        <div class="cell-lg-9">

            <div class="bg-white p-4 m-2">
                <h4>Registrar nueva Area</h4>

                <form class="custom-validation need-validation" novalidate="">

                    <div class="row mb-3">
                        <div class="cell-md-6">
                            <label>Nombre de Area</label>
                            <input type="text" disabled value="Soporte">

                        </div>

                        <div class="cell-md-6">
                            <label>Numero</label>

                            <input type="number" disabled value="2">
                        </div>
                        

                    </div>

                    <div class="row mb-3">
                        <div class="cell-md-6">
                            <label>Estado</label>

                            <input type="text" disabled value="Habilitado">
                        </div>
                    </div>





                        <!--Botones de Crear y volver atras-->
                      
                        <a href="@Url.Action("Index","Area")"> <input type="button" value="Atras"></a>

                </form>
            </div>
        </div>

    </div>


</body>
</html>

