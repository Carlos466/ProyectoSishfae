<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ver Tarea</title>


    <style type="text/css">
    </style>



</head>
<body>


    <div class="container">


        <div class="cell-lg-9">

            <div class="bg-white p-4 m-2">
                <h4>Detalle de Tarea</h4>

                <form class="custom-validation need-validation" novalidate="">

                    <div class="row mb-3">
                        <div class="cell-md-6">
                            <label>codigo Tarea</label>
                            <input type="number" disabled>
                            
                        </div>

                        <div class="cell-md-6">
                            <label>Nombre de Tarea</label>
                            <input type="text" disabled>
                        
                        </div>



                    </div>

                    <div class="row mb-3">
                        <!--Lista desplegable-->
                        <div class="cell-md-6">
                            <label>Estado</label>
                            <select disabled>
                                <option selected >Seleccione</option>
                                <option value="Habilitado">Habilitado</option>
                                <option value="En espera">En Espera</option>
                                <option value="Otro">Otro</option>
                            </select>
                        </div>

                        <div class="cell-md-6">
                            <label>Horas</label>
                            <input type="number" disabled>
                          
                        </div>

                    </div>

                    <div class="row mb-3">
                        <!--Lista desplegable-->
                        <div class="cell-md-6">
                            <label>Area</label>
                            <select disabled>
                                <option selected>Seleccione</option>
                                <option value="Habilitado">Area 1</option>
                                <option value="En espera">Area 2</option>
                                <option value="Otro">Otro</option>
                            </select>
                        </div>
                    </div>



                    <!--Botones de Crear y volver atras-->
                   
                
                    <a href="@Url.Action("Index","Tareas")"> <input type="button" value="Atras"></a>

                </form>
            </div>
        </div>

    </div>


</body>
</html>

